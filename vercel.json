{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd app && npm run build",
  "outputDirectory": "app/.next",
  "installCommand": "npm install && cd app && npm install",
  "devCommand": "cd app && npm run dev",
  "functions": {
    "app/app/api/zktls/config/route.ts": {
      "maxDuration": 30
    },
    "app/app/api/zktls/sign/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}